   "source": [
    "\n",
    "def find_common_letters(s1, s2):\n",
    "    set1 = set(s1)\n",
    "    set2 = set(s2)\n",
    "    common = set1.intersection(set2)\n",
    "    return sorted(list(common))\n",
    "\n",
    "S1 = \"likitha\"\n",
    "S2 = \"harshitha\"\n",
    "\n",
    "common_letters = find_common_letters(S1, S2)\n",
    "print(\"Common letters:\", common_letters)"
   ]
